<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dinehawaiipartner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.LoginActivity" />
        <activity
            android:name=".Activity.VendorHomeActivity"
            android:label="@string/title_activity_vendor_home"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Activity.DriverHomeActivity"
            android:label="@string/title_activity_driver_home"
            android:theme="@style/AppTheme" />
        <activity android:name=".Activity.ManageDriversActivity" />
        <activity android:name=".Activity.AddNewDriverActivity"></activity>
    </application>

</manifest>